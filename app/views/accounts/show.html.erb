<div class="col-lg-12 mt-5">
  <div class="card">
    <div class="card-body">
      <div class="col-12">
        <h4 class="mt-0 header-title">My Reimbursement</h4>
      </div>
      <div class="col-12">
        <hr class="my-4">
      </div>
      <table style="width:100%">
        <tr>
          <th>Submit Date</th>
          <th>Amount</th>
          <th>Subject Count</th>
          <th>Status</th>
          <th></th>
        </tr>
      <%@expenses.find_each do |expense| %>
        <tr>
          <td><%=expense.date%></td>
          <td><%=expense.amount%></td>
          <td><%=expense.people%></td>
          <td><%=expense.status%></td>
          <td><%=button_tag "review", class:'btn btn-outline-info' %></td>
        </tr>
      <%end%>
      </table>

    </div>
  </div>
</div>
